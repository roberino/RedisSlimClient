<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;netcoreapp3.1;net6.0</TargetFrameworks>
		<LangVersion>10.0</LangVersion>
		<Version>0.3.3-beta</Version>
		<Authors>Rob Eyres</Authors>
		<Company />
		<Description>JSON serialization extensions for RedisTribute</Description>
		<Copyright>R Eyres</Copyright>
		<PackageLicenseFile>LICENCE.txt</PackageLicenseFile>
		<PackageProjectUrl>https://github.com/roberino/RedisSlimClient/</PackageProjectUrl>
		<RepositoryUrl>https://github.com/roberino/RedisSlimClient/</RepositoryUrl>
		<PackageTags>Redis Client Cache JSON</PackageTags>
		<PackageIconUrl>https://raw.githubusercontent.com/roberino/RedisSlimClient/beta/docs/logo.png</PackageIconUrl>
	</PropertyGroup>
	<ItemGroup>
		<None Include="LICENSE.txt" Pack="true" PackagePath="$(PackageLicenseFile)" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="Utf8Json" Version="1.3.7" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\RedisTribute\RedisTribute.csproj" />
	</ItemGroup>
</Project>